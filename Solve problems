## Problem 2
#include <bits/stdc++.h>
#include <unordered_set>
#define  ll long long
# define joo ios_base::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
using namespace std;



int main() {
    joo;
  

    int degree;
    cout << "Enter the degree ";
    cin >> degree;

    if (degree >=85)
    {
        cout << "Excellent";
    }
    else if (degree >= 70)
    {
        cout << "Very Good";
    }
    else if(degree >=50) {
        cout << "Good";
    }
    else {
        cout << "Fail";
    }
    cout << endl;
          return 0;
    }
  
    



